
public class MyFun {
	public static void main(String[] args) {
	
	int a = 4;
	int b = 2;
	int sum = add(a,b);
	int min = minus(a,b);
	System.out.println(sum);
	System.out.println(min);
	
		
	}
	
	public static int add(int a, int b) {
		return a + b;
	}
	public static int minus(int a, int b) {
		return a - b;
	}
}
