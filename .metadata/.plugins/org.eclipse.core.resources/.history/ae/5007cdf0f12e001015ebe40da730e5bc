class Animal {
	constructor() {
		this.x = 0;
		this.y = 0;
	}

	move(x, y) {
		this.x = x;
		this.y = y;
	}
}

class Human extends Animal {
	constructor() {
		super();
		this.job = "student";
	}

	chibbo(job) {
		this.job = job;
	}
}

function main() {
	const hum = new Human();
	console.log(hum.x);

}